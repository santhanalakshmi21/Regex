import re
regex_pattern = r'hackerrank'
# Regex Pattern:
# hackerrank - Denotes the 'hackerrank' string
# Example: hackerrank
test_string = input()
match = re.findall(regex_pattern, test_string)
print("Number of matches :", len(match))Regex_Pattern = r' hackerrank'  # Do not delete 'r'.
import re
Test_String = input()
match = re.findall(Regex_Pattern, Test_String)
print("Number of matches :", len(match))
